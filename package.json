{
  "name": "nuxt-theme-liting",
  "type": "module",
  "version": "0.4.1",
  "private": false,
  "description": "a theme about nuxt",
  "author": "liting-yes <luz.liting@gmail.com>",
  "license": "MIT",
  "homepage": "https://nuxt-theme.liting.ink",
  "repository": {
    "type": "git",
    "url": "https://github.com/liting-yes/nuxt-theme-liting.git"
  },
  "keywords": [
    "theme",
    "nuxt",
    "blog",
    "vue"
  ],
  "main": "./nuxt.config.ts",
  "types": "./theme.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "assets/",
    "components/",
    "composables/",
    "layouts/",
    "public/",
    "server/",
    "stores/",
    "utiles/",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "theme.d.ts"
  ],
  "scripts": {
    "refresh": "nuxi prepare & nuxi prepare .playground",
    "cleanup": "nuxi cleanup .playground",
    "typecheck": "nuxi typecheck",
    "dev": "nuxi prepare & nuxi dev .playground",
    "build": "nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "postinstall": "nuxi prepare",
    "commit": "git-cz",
    "release": "release-it",
    "format": "prettier --write --list-different --ignore-unknown ."
  },
  "peerDependencies": {
    "execa": "^6.1.0",
    "nuxt": "^3.5.3"
  },
  "dependencies": {
    "@element-plus/nuxt": "^1.0.5",
    "@iconify/json": "^2.2.77",
    "@nuxt/content": "^2.7.0",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/google-fonts": "3.0.1",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/nuxt": "^0.53.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "animate.css": "^4.1.1",
    "date-fns": "^2.30.0",
    "element-plus": "^2.3.6",
    "execa": "^6.1.0",
    "lodash-es": "^4.17.21",
    "sass": "^1.63.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/glob": "^8.1.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.3",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "typescript": "^4.9.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown --list-different",
    "*.{ts,vue,md,json,yaml}": "eslint --fix"
  }
}
